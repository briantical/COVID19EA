{
  "name": "covid19ea",
  "version": "1.0.0",
  "description": "An application to track and notify the updates on Coronavirus in East Africa",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "cd client && yarn start",
    "server": "cd server && npm start",
    "client_install": "cd client && yarn install",
    "server_install": "cd server && yarn install",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "depend": "concurrently --kill-others-on-fail \"yarn server_install\" \"yarn client_install\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briantical/COVID19EA.git"
  },
  "keywords": [
    "Covid19ea",
    "Covid19",
    "Coronavirus"
  ],
  "author": "LUTAAYA Brian Ivan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/briantical/COVID19EA/issues"
  },
  "homepage": "https://github.com/briantical/COVID19EA#readme",
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.3.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2"
  },
  "devDependencies": {
    "jest": "^25.3.0"
  }
}
